            <content>*w3* = aaababba</content>
            <correct>false</correct>
            <id>0</id>
        </answer>
        <answer>
            <content>*w4* = bbababaaaba</content>
            <correct>true</correct>
            <id>0</id>
        </answer>
        <answer>
            <content>*w5* = ba</content>
            <correct>false</correct>
